        "thecodingmachine/phpstan-safe-rule": "^1.2",
        "phpunit/phpunit": "^9"
    },
    "require": {
        "thecodingmachine/safe": "2.2.x-dev",
        "squizlabs/php_codesniffer": "4.0.x-dev"
    }
}
